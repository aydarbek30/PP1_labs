#include <iostream>
//aydarbek

using namespace std;

int binary_search(int arr[], int left, int right, int key){
    int midd = 0;
    if(right >= left){
        int mid = left + (right - left) / 2;
        if(arr[mid] == key){
            return mid;
        }

        if(arr[mid] > key){
            return binary_search(arr, left, mid - 1, key);
        }


        return binary_search(arr, mid + 1, right, key);
    }
    return -1;
}

int main(){
    int n, k;
    cin >> n >> k;
    int a[n], b[k];
    for(int i = 0; i < n; i++){cin >> a[i];}
    for(int j = 0; j < k; j++){cin >> b[j];}

    for(int i = 0; i < k; i++){
        if(binary_search(a, 0, n - 1, b[i]) != -1){
            cout << "YES" << endl;
        }else{
            cout << "NO"  << endl;
        }
    }

    return 0;
}
